<div class="page-header">
  <h1>Pictures</h1>
</div>

<% @list_of_pictures.each do |picture| %>
<div class="row">
    <div class="col-sm-12">
      <div class="panel panel-default">
        <div class="panel-body">
          <div>
              <img src="<%= picture.source %>" width = "200">
          </div>
          <div>
            <p> <%= picture.caption %> </p>
          </div>
        </div>

      </div>
    </div>
</div>
<% end %>

